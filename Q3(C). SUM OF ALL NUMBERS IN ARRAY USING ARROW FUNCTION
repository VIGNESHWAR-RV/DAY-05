
     "SUM OF ALL NUMBERS IN ARRAY USING ARROW FUNCTION"
	 
 //ARROW FUNCTION
  
let array =[1,2,3,4,5,6];
let total = (num)=>{
    let total=0;
    for(let i=0;i<num.length;i++){
        total+=num[i];
    }return total;
};
console.log(total(array));
 
